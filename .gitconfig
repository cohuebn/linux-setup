[alias]
    co = checkout
    ci = commit
    br = branch
    st = status
    di = diff
    branch-name = "!git rev-parse --abbrev-ref HEAD"
    put = "!git push -u origin $(git branch-name)"
    rhu = "!git reset --hard origin/$(git branch-name)"
